CREATE TABLE movies (
  moive_id INT PRIMARY KEY,
  title VARCHAR(255)
  genre VARCHAR(255) 
  release_date VARCHAR(255)  
  producer_name VARCHAR(255)
);

CREATE TABLE users
  user_id INT PRIMARY KEY,
  username VARCHAR(255)
  email VARCHAR(255)
);

CREATE TABLE ratings (
  rating FLOAT,
  movie_id INT,
  user_id INT,
  FOREIGN KEY (movie_id) REFERENCES movies(movie_id)
);


  


